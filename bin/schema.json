[ 
        {
            "name": "Authenticate:",
            "endpoints": [
                {
                    "nickname": "Authenticate",
                    "description": "Validates user and sets access token. Must be run before any other calls.",
                    "notes": "<ul><li>Token session is good for 15 minutes.</li> <li>PrimaryResult is the token value that MUST be used in all subsequent requests</li></ul>",
                    "parameters": {
                        "qs": [
                            {
                                "name": "user",
                                "type": "string",
                                "description": "Your specific username is provided by cyntrxfleet.com"
                            }
                        ]
                    },
                    "uri": "https://service.cyntrxfleet.com:6443/rest/VehicleService.svc/user/{user}",
                    "verb": "get",
                    "verbLabel": "info",
                    "examples": {
                        "ex": [
                            {
                                "name": "Header",
                                "sample": "Authorization: <password> assigned by Cyntrx <br /> Content-Type: application/json <br /> Host: service.cyntrxfleet.com:6443"
                            },
                            {
                                "name": "Url",
                                "sample": "https://service.cyntrxfleet.com:6443/rest/VehicleService.svc/user/JSmith"
                            },
                            {
                                "name": "Response Format",
                                "sample": "JSON"
                            },
                            {
                                "name": "Response Text",
                                "sample": "[\"ErrorMessage\":null,\"PrimaryResult\":\"68c0bcdc-fde4-4c25-84e7-d5c6d2f9393c\",\"Success\":true,\"TransactionId\":\"003c2181-10f4-4a46-9ca3-5574ac911bf9\"]"
                            },
                            {
                                "name": "HTTP Method",
                                "sample": "GET"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "name": "GetHistory",
            "endpoints": [
                {
                    "nickname": "GetHistoryByDate",
                    "description": "Retrieves data from all company vehicles using a date range",
                    "notes": "Date and Time values; A \"T\" separates the entries for Date and Time. There is a maximum date range of 72 hours per request. If no end date is specified, 72 hours from start date will be used. ",
                    "parameters": {
                        "qs": [
                            {
                                "name": "utcStartDateTime",
                                "type": "string",
                                "description": "The date and time at which you would like the data to start (YYYY-MM-DDTHH-MM-SS)"
                            },
                            {
                                "name": "utcEndDateTime",
                                "type": "string",
                                "description": "The date and time at which you would like the data to end (YYYY-MM-DDTHH-MM-SS)"
                            }
                        ]
                    },
                    "uri": "https://service.cyntrxfleet.com:6443/rest/VehicleService.svc/history?start={utcStartDateTime}&end={utcEndDateTime}",
                    "verb": "get",
                    "verbLabel": "info",
                    "examples": {
                        "ex": [
                            {
                                "name": "Header",
                                "sample": "Authorization: <tolken> value from Authenticate (see above) <br /> Content-Type: application/json <br /> Host: service.cyntrxfleet.com:6443"
                            },
                            {
                                "name": "Url",
                                "sample": "https://service.cyntrxfleet.com:6443/rest/VehicleService.svc/history?start=2012-12-11T00:00:00&end=2012-12-12T23:59:59"
                            },
                            {
                                "name": "Response Format",
                                "sample": "JSON"
                            },
                            {
                                "name": "Response Text",
                                "sample": "{\"HistoryList\":[{\"Address\":\"County Road . . .\"}],\"ResultDetail\":[],\"Status\":{\"Code\":1,\"Description\":success},\"TransactionId\":\"ee55d1d2-403b-40de-98b8-fbbedd1b2311\"}"
                            },
                            {
                                "name": "HTTP Method",
                                "sample": "GET"
                            }
                        ]
                    }
                },
                {
                    "nickname": "GetHistoryBySeqNo",
                    "description": "Retrieves data from all company vehicles using a specified history sequence ID",
                    "notes": "A maximum of 1000 records will be returned per sequence number request.",
                    "parameters": {
                        "qs": [
                            {
                                "seqNo": "user",
                                "type": "string",
                                "description": "The history id that you would like to begin your query from"
                            }
                        ]
                    },
                    "uri": "https://service.cyntrxfleet.com:6443/rest/VehicleService.svc/history?{seqNo}",
                    "verb": "get",
                    "verbLabel": "info",
                    "examples": {
                        "ex": [
                            {
                                "name": "Header",
                                "sample": "Authorization: <tolken> value from Authenticate (see above) <br /> Content-Type: application/json <br /> Host: service.cyntrxfleet.com:6443"
